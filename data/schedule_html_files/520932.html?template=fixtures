    }
} 
else if (fb_cookies != null || globalFBConnect != false)
{
	
	var login_content = '<span style="color:#0D82E9; font-size:11px; padding-bottom:5px; padding-bottom:5px; margin:0;">Welcome <img src="http://i.imgci.com/espncricinfo/connect_fb_icon.png" src="Facebook Connect" style="position:relative; top:3px;"/> <b><fb:name id="fbname" uid="loggedinuser" useyou="false"></fb:name><br /><a href="javascript:void(0)" id="fbLogout" onclick="FB.logout(function(response){ globalFBConnect = false; removeCookie(\'fbsr_260890547115\',\'/\',getDomainname()); if($(\'#comment_frame\').length==1){ window.location.reload(true); return false; } $(\'#ciMemebermgmt\').html(welcome_content); FB.XFBML.parse(); });" style="top: 5px; position:relative; font-weight:normal; cursor:pointer;">Sign Out</a></b></span>';

	document.write(login_content);
}
else  
{
    document.write(welcome_content);
}

function updateMasthead(){
	var fb_cookies = getCookie("fbsr_260890547115");
	var authcookie = getCookie("CI_Pub_data");		
	
	updateUserInfo();
}
function updateUserInfo(){
	var fbNameCheck = $('#fbname').text();
	var txt = $('#newfb').text();
	FB.getLoginStatus(function(response) {
		if (response.status === 'connected'){
			if(fb_cookies || globalFBConnect){
				if(!authcookie && fbNameCheck.length==0){
					var welcome_content = '&nbsp;|&nbsp;<fb:login-button onlogin="window.location.reload(true);" autologoutlink="false" size="small" style="margin-left:2px;"></fb:login-button>';
					if($('#fbname').children('a').text() == ''){
						$('#ciMemebermgmt').html('<span style="color:#0D82E9; font-size:11px; padding-bottom:5px;">Welcome <img src="http://i.imgci.com/espncricinfo/connect_fb_icon.png" src="Facebook Connect" style="position:relative; top:3px;"/> <b><fb:name id="fbname" uid="loggedinuser" useyou="false"></fb:name><br /><a href="javascript:void(0)" id="fbLogout2" onclick="FB.logout(function(response){ globalFBConnect = false; removeCookie(\'fbsr_260890547115\',\'/\',getDomainname()); if($(\'#comment_frame\').length==1){ window.location.reload(true); } $(\'#ciMemebermgmt\').html(welcome_content); FB.XFBML.parse(); });" style="top: 5px; position:relative; font-weight:normal; cursor:pointer;">Sign Out</a></b></span>');
						FB.XFBML.parse()
					}
					if(txt.indexOf('Post')<0){
						$('#newfb').html('<span id="newfb"><a href="#comment_frame"><span style="color:#035BAC; margin-left:2px;">Post yours as</span></a> <img src="http://i.imgci.com/espncricinfo/connect_fb_icon.png" style="margin-right: 2px; vertical-align: bottom; border: medium none;"> <b><fb:name id="fbname" uid="loggedinuser" useyou="false"></fb:name></b> <span class="seprator">|</span> </span>');
					}		
					FB.XFBML.parse();
				}
			}
		}
		else{
			if(!fb_cookies && !authcookie && !globalFBConnect){
				$('#ciMemebermgmt').html('<div style="color:#0D82E9; font-size:11px; padding-bottom:5px; padding-bottom:5px; margin:0;">Welcome Guest </div><li class="signin"><a href="#" class="signintxt" onclick="javascript:return showTooltip(this,\'divLoginpopup\');">Sign In&nbsp;<blockquote class="signArrow">&nbsp;&nbsp;&nbsp;</blockquote></a></li>| <li class="signin" style="margin:0;"><a href="/member_mgmt/content/submit/member_mgmt/user_registration.html?sc=masthead" class="signintxt">Register</a></li>&nbsp;|&nbsp;<fb:login-button onlogin="window.location.reload(true);" autologoutlink="false" size="small" style="margin-left:2px;" show-faces="false"></fb:login-button>');
				FB.XFBML.parse();
			}
		}
	});
	if($('#fbname').children('a').text() == ''){	//Retry
		window.setTimeout(updateUserInfo,700);
	}
	else{
		if((navigator.userAgent).indexOf('MSIE')>0){	//IE hack to pouplate username
			var fb_un = $('#fbname').children('a').text();
			var fb_url = $('#fbname').children('a').attr('href'); 
			$('#newfb').html('<span id="newfb"><a href="#comment_frame"><span style="color:#035BAC; margin-left:2px;">Post yours as</span></a> <img src="http://i.imgci.com/espncricinfo/connect_fb_icon.png" style="margin-right: 2px; vertical-align: bottom; border: medium none;"> <b><a href="' + fb_url + '">' + fb_un + '</a></b> <span class="seprator">|</span> </span>');		
			$('.fbUserName').html('<b style="color: rgb(51, 51, 51); font-size:11px; margin-left:4px;">Logged in as <b><a href="' + fb_url + '">' + fb_un + '</a></b> <img style="margin-right:2px; margin-left:1px; position:relative; top:3px;" src="http://i.imgci.com/espncricinfo/connect_fb_icon.png" /> <a onclick="FB.logout(function(response){ globalFBConnect = false; removeCookie(\'fbsr_260890547115\',\'/\',getDomainname());  window.location.reload(true);});" style="font-weight:normal; cursor:pointer; color:#035BAC;">Sign Out</a></b>');
		}
		
	}

}
</script>

