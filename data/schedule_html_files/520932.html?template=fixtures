<script type="text/javascript">
var globalFBConnect = false;
$(document).ready(function(){
	FB.getLoginStatus(function(response) {
		if(response.status === 'connected'){
			globalFBConnect = true;
		}
		else{
			globalFBConnect = false;
		}
	});
	var fb_cookies = getCookie("fbsr_260890547115");
	var authcookie = getCookie("CI_Pub_data");
	if((fb_cookies || globalFBConnect) && !authcookie){
		updateMasthead();
	}
	
});		
var server = "submit.espncricinfo.com";
    
/* Login Valdation */

//----------------------------------------------------------
//      create cookie
//----------------------------------------------------------
function setCookie( name, value, expires, path, domain, secure ) {

var today = new Date();

    today.setTime( today.getTime() );
    if ( expires ) {
        expires = expires * 1000 * 60 * 60 * 24;
     }

     var expires_date = new Date( today.getTime() + (expires) );

     document.cookie = name+'='+escape( value ) + 
                       ( ( expires ) ? ';expires='+expires_date.toGMTString() : '' ) + 
                       ( ( path ) ? ';path=' + path : '' ) +
                       ( ( domain ) ? ';domain=' + domain : '' ) +
                       ( ( secure ) ? ';secure' : '' );

}

//----------------------------------------------------------
//      check if cookie defined 
//----------------------------------------------------------
function getCookie(name) {

    var start = document.cookie.indexOf( name + "=" );
    var len = start + name.length + 1;

    if ( ( !start ) && ( name != document.cookie.substring( 0, name.length ) ) ) {
       return null;
    }

    if ( start == -1 ) return null;
    var end = document.cookie.indexOf( ';', len );
    if ( end == -1 ) end = document.cookie.length;
    return unescape( document.cookie.substring( len, end ) );
}

//------------------------------------------------------------
//      Remove cookie
//------------------------------------------------------------
function removeCookie(  name, path, domain) {
    if ( getCookie( name ) )
	{
        document.cookie = name + '=' + ( ( path ) ? ';path=' + path : '') + ( ( domain ) ? ';domain=' + domain : '' ) + ';expires=Thu, 01-Jan-1970 00:00:01 GMT';
	}
}
//------------------------------------------------------------
//      Get the domain name
//------------------------------------------------------------
function getDomainname () {
    var hostname = window.location.hostname.split('.');
    if ( hostname.length >= 2 ) {
        var len = hostname.length;
        var domainname = '.' +  hostname[len - 2] + '.' + hostname[len - 1];
    } else {
        var domainname = '.' + window.location.hostname;
    }
    return domainname;
}

//------------------------------------------------------------
//      Logout
//------------------------------------------------------------
function logoutUser () {
    var domainname = getDomainname();
    removeCookie("un" , '/', domainname );
    removeCookie("CI_Pub_data" , '/' , domainname);
    removeCookie("uid" , '/', domainname );

	window.location.reload(true);


	return false;
    $('#ciMemebermgmt').html(welcome_content);


	
}

//-------------------------------------------------------------
//      Validate search filed
//-------------------------------------------------------------
function validateData() {
	if ( txt_data == "") {
		window.location.href = "/ci/content/site/search.html";
		return false;
	}
	if ( txt_data == "Search Cricinfo") {
		window.location.href = "/ci/content/site/search.html";
		return false;
	}
	return true;
}
//----------------------------------------------------------
//  Login validate
//----------------------------------------------------------
function ciLoginvalidate(){

  if(document.ciloginfm.email.value=="") {
    alert("Please enter your email id.");
    document.ciloginfm.email.focus();
    document.ciloginfm.email.select();
    return false;
  }
  var emailexp = /^([a-zA-Z0-9])([a-zA-Z0-9_\-\.]+)@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3})|([a-zA-Z0-9\-\.]+))\.([a-zA-Z]{2,6}|[0-9]{1,6})(\]?)$/;
  if (emailexp.test(document.ciloginfm.email.value.replace(/\s/g,""))== false) {
    alert("Please enter your email id.");
    window.document.ciloginfm.email.focus();
    document.ciloginfm.email.select();
    return false;
  }
  if(document.ciloginfm.password.value=="") {
    alert("Please enter Password.");
    document.ciloginfm.password.focus();
    document.ciloginfm.password.select();
    return false;
  }

  if(document.ciloginfm.remember.checked) {
    var rem = 'Y';
  } else {
    var rem = 'N';
  }



  var un = document.ciloginfm.email.value;
  var pass =document.ciloginfm.password.value;
  var url = 'http://' + server + '/member_mgmt/content/submit/member_mgmt/login_validate.html?callback=?';
  $.getJSON( url , { email: un, password: pass, remember: rem },
        function(msg){
            if ( msg.status == 1 ) {
                if ( msg.username == "" ) {
                    var href = 'http://' + server + '/member_mgmt/content/submit/member_mgmt/user_screenname.html?remember=' + rem + ';thanks=' + "http://www.espncricinfo.com/indian-premier-league-2012/content/series/520932.html"
		            window.location.href = href;
                    return false;
                }
                hideTooltip('divLoginpopup');
                var welcome_user = '<ul><li>Welcome <b>' + msg.username + '</b></li><li class="signin"><a href="/member_mgmt/content/submit/member_mgmt/user_registration.html" class="signintxt">Edit Profile</a></li> | <li class="signin"> <a href="#" class="signintxt"> <span onClick="logoutUser();">Sign Out</span> </a> </li> </ul>';

    		$('#ciMemebermgmt').html(welcome_user);
    

           } else {
                alert (msg.message);
                return false;
           }
          });
   return false;
}


</script>

