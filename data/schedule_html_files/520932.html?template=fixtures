<script language="JavaScript">

function startclock(){
	var thetime=new Date();
	var nhours=thetime.getUTCHours();
	var nmins=thetime.getUTCMinutes();
	var nsecn=thetime.getUTCSeconds();

	var AorP=" ";
	if (nhours>=24)
    nhours-=12;

	if (nhours==0)
   	nhours=12;

	if (nsecn<10)
 	nsecn="0"+nsecn;
	if (nmins<10)
	 nmins="0"+nmins;
	document.ciClockform.gmtClock.value=nhours+": "+nmins+": "+nsecn;
	setTimeout('startclock()',1000);
} 

</script>

