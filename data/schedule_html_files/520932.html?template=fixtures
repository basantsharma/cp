<script type="text/javascript"> 
$(function () { 
    $(".srstabHldr li").bind("click", function () { 
        var i = $(this).attr("id"); 
        if (!$(this).hasClass("current")) { 
            $(this).siblings().removeClass("current"); 
            $(this).addClass("current"); 
        } 
        $(this).parents("div.srstabHldr").siblings("div.cont").children("div").css("display", "none"); 
        $(this).parents("div.srstabHldr").siblings("div.cont").children("div#" + i + "-content").css("display", "block"); 
    }); 
        $(".standings-nav a").bind("click", function () { 
        var i = $(this).attr("id"); 
        if (!$(this).hasClass("mstbactive")) { 
            $(this).siblings().removeClass("mstbactive"); 
            $(this).addClass("mstbactive"); 
        } 
        $(this).parents("div.standingsGutter").siblings("div.latest").children("div").css("display", "none"); 
        $(this).parents("div.standingsGutter").siblings("div.latest").children("div#" + i + "-content").css("display", "block"); 
                $(this).parents("div.standingsGutter").siblings("div.mostvwd").children("div").css("display", "none"); 
        $(this).parents("div.standingsGutter").siblings("div.mostvwd").children("div#" + i + "-content").css("display", "block"); 
                $(this).parents("div.standingsGutter").siblings("div.mostdsd").children("div").css("display", "none");
        $(this).parents("div.standingsGutter").siblings("div.mostdsd").children("div#" + i + "-content").css("display", "block");
    });
});
function changedays(tabname, camefrom) {
	if (tabname == '1') {
		if($("#mostviw-2").attr("class") == "mstbactive") {
			$(".days").html("Last 7 days");
		} else {
			$(".days").html("Last 3 days");
		}
	} else if (tabname == '2') {
		if($("#mostdisd-2").attr("class") == "mstbactive") {
			$(".days").html("Last 7 days");
		} else {
			$(".days").html("Last 3 days");
		}
	} else if(tabname == "news") {
                if (camefrom == "mostv") {
                        $("#mostviw-1").attr("class", "mstbactive");
                        $("#mostviw-2").attr("class", "inactive");
                } else if (camefrom == "mostd") {
                        $("#mostdisd-1").attr("class", "mstbactive");
                        $("#mostdisd-2").attr("class", "inactive");
                }
		$(".days").html("Last 3 days");
	} else if (tabname == "specials") {
		if (camefrom == "mostv") {
			$("#mostviw-1").attr("class", "inactive");
			$("#mostviw-2").attr("class", "mstbactive");
		} else if (camefrom == "mostd") {
                        $("#mostdisd-1").attr("class", "inactive");
                        $("#mostdisd-2").attr("class", "mstbactive");
		}
		$(".days").html("Last 7 days");
	}
}
</script>
